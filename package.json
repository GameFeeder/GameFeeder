{
  "name": "gamefeeder",
  "version": "0.18.0",
  "description": "A notification bot for several games, available on Discord and Telegram.",
  "main": "src/_main.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GameFeeder/GameFeeder"
  },
  "author": "Tim Jentzsch",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/GameFeeder/GameFeeder/issues"
  },
  "homepage": "https://github.com/GameFeeder/GameFeeder",
  "engines": {
    "node": ">=18.12.1",
    "npm": ">=8.19"
  },
  "scripts": {
    "build": "tsc --build",
    "dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"ts-node src/_main.ts\"",
    "start": "node dist/src/_main.js",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.spec.ts\"",
    "format": "eslint \"src/**/*.ts\" --fix",
    "test": "jest --config jest.config.json",
    "coverage": "jest --config jest.config.json --coverage",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "discord.js": "14.7.1",
    "escape-string-regexp": "4.0.0",
    "lodash": "4.17.21",
    "node-fetch": "2.6.7",
    "node-html-parser": "3.3.6",
    "pubsub-js": "1.9.4",
    "rss-parser": "3.12.0",
    "smart-request-balancer": "2.1.1",
    "snoowrap": "1.23.0",
    "telegraf": "4.3.0",
    "turndown": "7.0.0",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/jest": "26.0.24",
    "@types/lodash": "4.14.171",
    "@types/node": "18.11.14",
    "@types/node-fetch": "2.5.12",
    "@types/pubsub-js": "1.8.2",
    "@types/request": "2.48.6",
    "@types/turndown": "5.0.1",
    "@types/ws": "7.4.7",
    "@types/xml2js": "0.4.9",
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "5.46.1",
    "eslint": "7.31.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jest": "24.4.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "nodemon": "2.0.12",
    "prettier": "2.3.2",
    "ts-jest": "27.0.4",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  }
}
