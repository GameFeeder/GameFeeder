{
  "name": "gamefeeder",
  "version": "0.17.7",
  "description": "A notification bot for several games, available on Discord and Telegram.",
  "main": "src/_main.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GameFeeder/GameFeeder"
  },
  "author": "Tim Jentzsch",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/GameFeeder/GameFeeder/issues"
  },
  "homepage": "https://github.com/GameFeeder/GameFeeder",
  "engines": {
    "node": ">=12",
    "yarn": ">=1.16.0"
  },
  "scripts": {
    "build": "tsc --build",
    "dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"ts-node src/_main.ts\"",
    "start": "node dist/src/_main.js",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.spec.ts\"",
    "format": "eslint \"src/**/*.ts\" --fix",
    "test": "jest --config jest.config.json",
    "coverage": "jest --config jest.config.json --coverage",
    "watch": "tsc --watch",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/lodash": "4.14.168",
    "@types/jest": "26.0.23",
    "@types/node": "14.14.43",
    "@types/node-fetch": "2.5.10",
    "@types/pubsub-js": "1.8.2",
    "@types/request": "2.48.5",
    "@types/turndown": "5.0.0",
    "@types/ws": "7.4.2",
    "@types/xml2js": "0.4.8",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "eslint": "7.25.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "nodemon": "2.0.7",
    "prettier": "2.2.1",
    "ts-jest": "26.5.5",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  },
  "dependencies": {
    "discord.js": "12.5.3",
    "escape-string-regexp": "5.0.0",
    "lodash": "4.17.21",
    "node-fetch": "2.6.1",
    "node-html-parser": "3.3.5",
    "pubsub-js": "1.9.3",
    "rss-parser": "3.12.0",
    "smart-request-balancer": "2.1.1",
    "snoowrap": "1.22.0",
    "telegraf": "4.3.0",
    "turndown": "7.0.0",
    "winston": "3.3.3"
  }
}
