{
  "name": "gamefeeder",
  "version": "0.23.0",
  "description": "A notification bot for several games, available on Discord and Telegram.",
  "main": "src/_main.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GameFeeder/GameFeeder"
  },
  "author": "Tim Jentzsch",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/GameFeeder/GameFeeder/issues"
  },
  "homepage": "https://github.com/GameFeeder/GameFeeder",
  "engines": {
    "node": ">=v22.21.1",
    "npm": ">=8.19"
  },
  "scripts": {
    "build": "tsc --build",
    "dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"node --loader ts-node/esm src/_main.ts\"",
    "start": "node --loader commonjs-extension-resolution-loader dist/src/_main.js",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.spec.ts\"",
    "format": "eslint \"src/**/*.ts\" --fix",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "commonjs-extension-resolution-loader": "0.1.0",
    "discord.js": "14.21.0",
    "escape-string-regexp": "5.0.0",
    "lodash": "4.17.21",
    "node-fetch": "3.3.2",
    "pubsub-js": "1.9.5",
    "rss-parser": "3.13.0",
    "smart-request-balancer": "2.2.0",
    "snoowrap": "1.23.0",
    "telegraf": "4.16.3",
    "turndown": "7.2.1",
    "winston": "3.13.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.8",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.25.1",
    "@jest/globals": "^30.0.0",
    "@types/jest": "30.0.0",
    "@types/lodash": "4.17.20",
    "@types/node": "22.18.6",
    "@types/node-fetch": "2.6.13",
    "@types/pubsub-js": "1.8.6",
    "@types/request": "2.48.13",
    "@types/turndown": "5.0.5",
    "@types/ws": "8.18.1",
    "@types/xml2js": "0.4.14",
    "@typescript-eslint/eslint-plugin": "8.44.1",
    "@typescript-eslint/parser": "8.44.1",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "29.0.1",
    "eslint-plugin-prettier": "5.5.4",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "nodemon": "3.1.10",
    "prettier": "3.6.2",
    "ts-jest": "29.4.4",
    "ts-node": "10.9.2",
    "typescript": "5.9.2"
  }
}
