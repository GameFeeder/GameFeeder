{
  "name": "gamefeeder",
  "version": "0.18.3",
  "description": "A notification bot for several games, available on Discord and Telegram.",
  "main": "src/_main.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GameFeeder/GameFeeder"
  },
  "author": "Tim Jentzsch",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/GameFeeder/GameFeeder/issues"
  },
  "homepage": "https://github.com/GameFeeder/GameFeeder",
  "engines": {
    "node": ">=18.12.1",
    "npm": ">=8.19"
  },
  "scripts": {
    "build": "tsc --build",
    "dev": "NODE_OPTIONS='--experimental-specifier-resolution=node' nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"ts-node src/_main.ts\"",
    "start": "NODE_OPTIONS='--experimental-specifier-resolution=node' node dist/src/_main.js",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.spec.ts\"",
    "format": "eslint \"src/**/*.ts\" --fix",
    "test": "NODE_OPTIONS='--experimental-vm-modules --experimental-specifier-resolution=node' jest --config jest.config.ts",
    "coverage": "NODE_OPTIONS='--experimental-vm-modules --experimental-specifier-resolution=node' jest --config jest.config.ts --coverage",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "discord.js": "14.7.1",
    "escape-string-regexp": "5.0.0",
    "lodash": "4.17.21",
    "node-fetch": "3.3.0",
    "node-html-parser": "6.1.4",
    "pubsub-js": "1.9.4",
    "rss-parser": "3.12.0",
    "smart-request-balancer": "2.1.1",
    "snoowrap": "1.23.0",
    "telegraf": "4.3.0",
    "turndown": "7.0.0",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@types/jest": "26.0.24",
    "@types/lodash": "4.14.191",
    "@types/node": "18.11.18",
    "@types/node-fetch": "2.6.2",
    "@types/pubsub-js": "1.8.3",
    "@types/request": "2.48.8",
    "@types/turndown": "5.0.1",
    "@types/ws": "7.4.7",
    "@types/xml2js": "0.4.11",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "24.7.0",
    "eslint-plugin-prettier": "3.4.1",
    "husky": "7.0.4",
    "jest": "29.4.0",
    "nodemon": "2.0.20",
    "prettier": "2.8.3",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  }
}
