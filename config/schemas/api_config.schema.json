      "additionalProperties": {
        "type": "object",
        "properties": {
          "autostart": {
            "type": "boolean"
          },
          "prefix": {
            "type": "string"
          },
          "token": {
            "type": "string"
          },
          "owners": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "enabled": {
            "type": "boolean"
          }
        },
